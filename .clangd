CompileFlags:
  Add: [
    "--cuda-path=/usr/local/cuda-12.5",
    "--no-cuda-version-check",
    "-I/usr/include/c++/11",  # Adjust according to your compiler version and path
    "-I/usr/include/x86_64-linux-gnu/c++/11",
    "-I/usr/lib/gcc/x86_64-linux-gnu/11/include",
    "-I/usr/local/include",
    "-I/usr/lib/llvm-11/include",
    "-I/usr/include",
  ]
  Remove: [
    "--gpu-architecture=*",
    "-G",
  ]
